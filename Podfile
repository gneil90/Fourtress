# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

# Comment the next line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

workspace 'Fourtress.xcworkspace'

target 'Fourtress' do

  # Pods for Fourtress
  project 'Fourtress.xcodeproj'

  pod 'RxSwift'
end

target 'NestSDK' do
  #Pods for NestSDK
  
  project 'NestSDK/NestSDK.xcodeproj'
  
  pod 'JSONModel'
  pod 'ReactiveObjC'
  pod 'MBProgressHUD'
  pod 'MagicalRecord'
end
